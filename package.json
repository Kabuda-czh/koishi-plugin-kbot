    "pub": "yakumo publish",
    "dev": "cross-env NODE_ENV=development koishi start -r esbuild-register -r yml-register --watch --experimental-vm-modules",
    "start": "koishi start --experimental-vm-modules",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "dependencies": {
