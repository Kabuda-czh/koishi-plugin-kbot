  "private": true,
  "license": "AGPL-3.0",
  "files": [
    ".env",
    "koishi.yml"
  ],
  "workspaces": [
    "external/*",
    "external/*/external/*",
    "external/*/external/*/packages/*",
    "external/*/external/*/packages/@*/*",
    "external/*/packages/*",
    "external/*/packages/@*/*",
    "packages/*",
    "packages/@*/*",
    "plugins/*",
    "plugins/@*/*"
  ],
  "scripts": {
    "new": "koishi-scripts new",
    "setup": "koishi-scripts setup",
    "build": "yakumo build",
    "bump": "yakumo version",
